{
    "workbench.startupEditor": "none",
    "python.showStartPage": false,
    "kite.showWelcomeNotificationOnStartup": false,
    "git.ignoreLegacyWarning": true,
    "editor.minimap.enabled": false,
    "explorer.confirmDragAndDrop": false,
    "[python]": {
        "editor.defaultFormatter": "ms-python.python"
    },
    "tabnine.experimentalAutoImports": true,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "explorer.confirmDelete": false,
    "docker.showStartPage": false,
    "security.workspace.trust.untrustedFiles": "open",
    "jupyter.jupyterServerType": "local",
    "jupyter.allowUnauthorizedRemoteConnection": true,
    "google.drive.alertMissingCredentials": false,
    "github-enterprise.uri": "vscode://vscode.github-authentication/did-authenticate?windowid=1&code=43f6f34f7788c032a722&state=9e8345b3-ced1-45fe-9767-637af08e7781",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "Darcula Pycharm with Dark GUI",
    "files.autoSave": "onFocusChange",
    "python.linting.enabled": true,
    "python.linting.pylintPath": "pylint",
    "python.linting.pylintArgs": [
        "--rcfile=${env:HOME}.pylintrc"
    ],
    "editor.formatOnSave": true,
    "python.formatting.provider": "yapf", // or "black" here
    "python.linting.pylintEnabled": true
}
